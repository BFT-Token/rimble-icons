{
  "name": "@rimble/icons",
  "version": "0.0.1",
  "description": "Rimble Icons",
  "main": "index.js",
  "module": "index.es.js",
  "sideEffects": false,
  "scripts": {
    "prepare": "./build.js && npx @svgr/cli -d ./source/tokens ./svg/tokens && npx @svgr/cli -d ./source/md ./svg/md && ./colorize.js && create-index ./source -ri && npm run build:cjs && npm run build:es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel source -d lib",
    "build:es": "babel source -d es",
    "clean": "rm -r svg source/md source/tokens lib es examples",
    "start": "cd demo && yarn start"
  },
  "keywords": [],
  "author": "Rimble",
  "license": "MIT",
  "dependencies": {
    "@styled-system/theme-get": "^5.1.2",
    "@svgr/cli": "^4.3.3",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "grid-styled": "^4.1.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.uniqby": "^4.7.0",
    "lodash.upperfirst": "^4.3.1",
    "material-design-icons": "^3.0.1",
    "ok-css": "^1.0.0-0",
    "pixo": "^1.1.0",
    "react": "^16.3.2",
    "react-test-renderer": "^16.3.2",
    "recursive-readdir": "^2.2.2",
    "styled-system": "^5.1.4"
  },
  "peerDependencies": {
    "styled-components": ">= 4"
  },
  "pixo": {
    "outDir": "source",
    "template": "styledSystem",
    "index": false,
    "iconComponent": true
  },
  "x0": {
    "cssLibrary": "styled-components"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "create-index": "^2.5.0",
    "cross-env": "^6.0.3"
  }
}
